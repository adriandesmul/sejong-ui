{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": [
			"dom",
			"es5",
			"es2015.promise"
		],
		"esModuleInterop": true,
		"importHelpers": true,
		"removeComments": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"declaration": true,
		"jsx": "react",
		"noImplicitReturns": true,
		"noImplicitThis": false,
		"noFallthroughCasesInSwitch": true
	}
}